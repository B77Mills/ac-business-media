$ const {
  id,
  alias,
  name,
  pageNode
} = data;

<refresh-theme-website-section-published-content-layout
  id=id
  alias=alias
  name=name
  page-node=pageNode
>
  <@query
    name="all-published-content"
    params={
      contentTypes: ["Ebook"],
      limit: 12,
    }
  />
</refresh-theme-website-section-published-content-layout>
