{
  "<website-content-card-node>": {
    "template": "./content-card.marko",
    "@node": "object"
  },
  "<website-content-hero-card-node>": {
    "template": "./content-hero-card.marko",
    "@node": "object"
  },
  "<website-magazine-latest-issue-node>": {
    "template": "./magazine-latest-issue.marko",
    "@node": "object",
    "@flush": "boolean",
    "@card": "boolean",
    "@image-width": {
      "type": "number",
